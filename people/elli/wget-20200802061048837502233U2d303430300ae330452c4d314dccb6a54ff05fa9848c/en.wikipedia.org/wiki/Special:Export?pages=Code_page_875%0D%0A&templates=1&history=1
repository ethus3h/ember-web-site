{{character encoding}}

[[Category:EBCDIC code pages]]</text>
      <sha1>2edxoh7qv7cls93fjyqxu6be17z4a5d</sha1>
    </revision>
    <revision>
      <id>950549641</id>
      <parentid>935599943</parentid>
      <timestamp>2020-04-12T17:26:57Z</timestamp>
      <contributor>
        <username>BD2412</username>
