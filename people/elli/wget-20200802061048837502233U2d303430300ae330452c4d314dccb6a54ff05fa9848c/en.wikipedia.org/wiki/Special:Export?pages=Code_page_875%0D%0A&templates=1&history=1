! width="6%" | —4 || width="6%" | —5
! width="6%" | —6 || width="6%" | —7
! width="6%" | —8 || width="6%" | —9
! width="6%" | —A || width="6%" | —B
! width="6%" | —C || width="6%" | —D
! width="6%" | —E || width="6%" | —F
